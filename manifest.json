{
  "manifest_version": 3,
  "name": "TikTok URLs extractor",
  "description": "Extracts TikTok URLs from the current page",
  "version": "1.0",
  "permissions": ["downloads", "tabs", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.tiktok.com/*"],
      "js": ["src/get_links.js"],
      "css": ["src/main.css"]
    }
  ]
}
